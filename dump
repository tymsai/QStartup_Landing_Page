    <script>
        $(document).ready(function () {
            $('input[type="reset"').click(function () {
                var inputValue = $(this).attr("value");
                var targetBox = $("." + inputValue);
                $(".box").not(targetBox).hide();
                $(targetBox).show();
            });
        });</script>

